<Window x:Class="Parking.Admin.AdminUserMain"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Parking.Admin"
        mc:Ignorable="d"
        Title="AdminUserMain" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                <GradientStop Color="Black" Offset="0"/>
                <GradientStop Color="#FF603A3A" Offset="1"/>
            </LinearGradientBrush>
        </Grid.Background>
        <StackPanel>
            <TextBlock FontSize="18" Foreground="Red" HorizontalAlignment="Center" Margin="0 10 0 50">Пользователи</TextBlock>
            <Button Margin="0 10 0 20" Content="Регистрация пользователя" Background="#FFB73A3A" BorderBrush="#FFC3BECD" Click="Regist_Click"></Button>
            <Button Margin="0 10 0 20" Content="Найти пользователя" Background="#FFB7743A" BorderBrush="#FFC1BCCB" Click="Find_Click"></Button>
        </StackPanel>
    </Grid>
</Window>
